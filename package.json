    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.49.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/greensock": "^1.15.32",
    "@types/node": "^22",
    "@types/three": "^0.182.0",
    "dprint": "^0.51.1",
    "eslint": "^9.39.1",
    "eslint-plugin-svelte": "^3.13.1",
    "globals": "^16.5.0",
    "gsap": "^3.14.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "sass-embedded": "^1.97.2",
    "svelte": "^5.45.6",
    "svelte-check": "^4.3.4",
    "three": "^0.182.0",
    "threlte": "^3.13.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": ["eslint --fix", "dprint fmt"],
    "**/*.{json,scss,html}": ["dprint fmt"]
